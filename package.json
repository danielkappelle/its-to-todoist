    "dotenv": "^16.3.1",
    "node-cron": "^3.0.3",
    "node-html-parser": "^6.1.11",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node-cron": "^3.0.11",
    "@types/puppeteer": "^7.0.4",
    "prettier": "^3.1.1",
    "type-fest": "^4.8.3",
    "typescript": "^5.3.3"
  }
